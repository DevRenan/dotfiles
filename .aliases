#!/bin/bash

[ "`uname`" == "Linux" ] && alias ls="ls --color"

alias ll='ls -aGlFh'
alias sl='ll'
alias l='ll'
alias snv='svn'

alias quit='exit'
alias :q='exit'

alias myip='curl -s http://ip.appspot.com'
alias chrome='open -a /Applications/Google\ Chrome.app'
alias reload='source ~/.bash_profile'
